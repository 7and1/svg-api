{
  "name": "@svg-api/worker",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev src/index.ts",
    "build": "wrangler build",
    "deploy": "wrangler deploy --env production",
    "deploy:preview": "wrangler deploy --env preview",
    "lint": "eslint src",
    "test": "vitest run",
